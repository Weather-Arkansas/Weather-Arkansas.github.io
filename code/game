<script>
// Center scaled canvas on screen using CSS3
var Game = {
	init: function () {
		// Get DOM elements
		this.element = document.getElementById('game');
		this.canvas = document.getElementById('canvas');
		// Original content size
		this.content = [this.canvas.width, this.canvas.height];
		// Reflow canvas size/margin on resize
		if(window.addEventListener){
			window.addEventListener('resize', this, false);
			window.addEventListener('orientationchange', this, false);
		}
		else {
			window.attachEvent('onresize', this, false);
		}
		this.reflow();
	},
	reflow: function () {
		var browser = [window.innerWidth, window.innerHeight];
		// Minimum scale to fit whole canvas
		var scale = this.scale = Math.min(browser[0] / this.content[0], browser[1] / this.content[1]);
		// Scaled content size
		var size = [this.content[0] * scale, this.content[1] * scale];
		// Offset from top/left
		var offset = this.offset = [(browser[0] - size[0]) / 2, (browser[1] - size[1]) / 2];
		// Apply CSS transform
		if(!isNaN(size[1]) && !isNaN(offset[0])) {
			this.canvas.style.width = size[0] + 'px';
			this.canvas.style.height = size[1] + 'px';
			this.element.style.marginLeft = offset[0] + 'px';
		}
	},
	// Handle all events
	handleEvent: function (event) {
		if(event.type == 'resize' || event.type == 'orientationchange') {
			this.reflow();
		}
	}
};
Game.init();
</script>
